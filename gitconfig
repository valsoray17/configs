[credential]
    helper = libsecret
[core]
    autocrlf = input
    excludesfile = ~/.gitignore_global
[alias]
    co = checkout
    ci = commit
    amend = commit --amend
    st = status
    br = branch
    lol = log --graph --decorate --oneline
    unstage = reset HEAD --
    last = log -1 HEAD
    a = add
    purr = pull --rebase
[include]
    path = ~/.gitconfig_local
